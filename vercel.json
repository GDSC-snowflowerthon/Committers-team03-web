{
  "rewrites": [{"source": "/(.*)", "destination": "/"}],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ],
  "functions": {
    "api/*.js": {
      "memory": 128,
      "maxDuration": 10
    }
  }

}
